{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "includePattern": "\\.(vue|js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongNameInNav": false,
    "showInheritedInNav": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/minami"
  }
}
